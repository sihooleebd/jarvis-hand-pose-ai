{
    "env": {
        "node":true, 
        "browser": true,
        "es2022": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "rules": {
        //"indent": [2, 2]
        "no-unused-vars"  : "warn"
    },
    "ignorePatterns": [
        "dist/",
        "node_modules/"
    ]
}